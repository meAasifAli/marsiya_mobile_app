{
  "expo": {
    "name": "کشمیری مرثیہ",
    "slug": "marsiya",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/panjtan.png",
    "scheme": "marsiya",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/panjtan.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/favicon.png"
    },
    "splash": {
      "image": "./src/assets/images/panjtan.png",
      "resizeMode": "contain",
      "backgroundColor": "#1A1A2E",
      "imageWidth": 200,
      "imageHeight": 200,
      "androidSplashResourceName": "splash",
      "androidScaleType": "CENTER_CROP"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/panjtan.png",
          "resizeMode": "contain",
          "backgroundColor": "#1A1A2E",
          "imageWidth": 200,
          "imageHeight": 200,
          "androidSplashResourceName": "splash",
          "androidScaleType": "CENTER_CROP"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}